#sidebar-collapse.col-sm-3.col-lg-2.sidebar
  form(role='search')
    .form-group
      input.form-control(type='text', placeholder='Search')
  ul.nav.menu
    li(ng-class='vm.ngclass.dashboardClass', ng-click="vm.goPage('dashboard');")
      a(href='')
        svg.glyph.stroked.dashboard-dial
          use(xlink:href='#stroked-dashboard-dial')
        |  Dashboard
    li(ng-class='vm.ngclass.institutionsClass', ng-click="vm.goPage('institutions');")
      a(href='')
        svg.glyph.stroked.calendar
          use(xlink:href='#stroked-calendar')
        |  Instituições
    li(ng-class='vm.ngclass.themesClass', ng-click="vm.goPage('themes');")
      a(href='')
        svg.glyph.stroked.line-graph
          use(xlink:href='#stroked-line-graph')
        |  Temas
    li(ng-class='vm.ngclass.skillsClass', ng-click="vm.goPage('skills');")
      a(href='')
        svg.glyph.stroked.table
          use(xlink:href='#stroked-table')
        |  Competências
    li(ng-class='class')
      a(href='#/')
        svg.glyph.stroked.pencil
          use(xlink:href='#stroked-pencil')
        |  Funcionalidade
    li.parent
      a(href='')
        span(data-toggle='collapse', href='#sub-item-1')
          svg.glyph.stroked.chevron-down
            use(xlink:href='#stroked-chevron-down')
        |  Dropdown
      ul#sub-item-1.children.collapse
        li
          a(href='#')
            svg.glyph.stroked.chevron-right
              use(xlink:href='#stroked-chevron-right')
            |  Sub Item 1
        li
          a(href='#')
            svg.glyph.stroked.chevron-right
              use(xlink:href='#stroked-chevron-right')
            |  Sub Item 2
        li
          a(href='#')
            svg.glyph.stroked.chevron-right
              use(xlink:href='#stroked-chevron-right')
            |  Sub Item 3
