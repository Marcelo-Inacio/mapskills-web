section
  .row
    .col-lg-3.col-md-3.col-sm-6.col-xs-6.col-lg-offset-2
      .panel.panel-default(ng-if="admDashCtrl.hasResult.Etecs")
        .panel-heading {{admDashCtrl.array[0].level}}
        .panel-body.panel-radar
          canvas#pie.chart.chart-pie(chart-data='admDashCtrl.array[0].values', chart-labels='admDashCtrl.labels', chart-options='admDashCtrl.options')
      .panel.panel-default(ng-if="!admDashCtrl.hasResult.Etecs")
        .panel-heading(style="text-align: center") Nenhum resultado
    .col-lg-3.col-md-3.col-sm-6.col-xs-6.col-lg-offset-1
      .panel.panel-default(ng-if="admDashCtrl.hasResult.Fatecs")
        .panel-heading {{admDashCtrl.array[1].level}}
        .panel-body.panel-radar
          canvas#pie.chart.chart-pie(chart-data='admDashCtrl.array[1].values', chart-labels='admDashCtrl.labels', chart-options='admDashCtrl.options')
      .panel.panel-default(ng-if="!admDashCtrl.hasResult.Fatecs")
        .panel-heading(style="text-align: center") Nenhum resultado
